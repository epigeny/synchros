{
  "type": "object",
  "properties": {
    "website": {
      "title": "t(website)",
      "type": "string"
    },
    "dataEntryDate": {
      "title": "t(initiative.dataEntryDate.title)",
      "type": "string",
      "format": "datepicker"
    },
    "yearCreated": {
      "title": "t(initiative.yearCreated.title)",
      "type": "number"
    },
    "typesOfCohorts": {
      "title": "t(initiative.typesOfCohorts.title)",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "population_cohort",
          "patient_cohort",
          "clinical_trial"
        ]
      }
    },
    "countries": {
      "type": "array",
      "format": "obibaCountriesUiSelect",
      "title": "t(initiative.countries.title)"
    },
    "region": {
      "title": "t(initiative.region.title)",
      "type": "string"
    },
    "setting": {
      "type": "string",
      "title": "t(initiative.setting.title)",
      "enum": [
        "regional",
        "national",
        "international"
      ]
    },
    "funding": {
      "title": "t(initiative.funding.title)",
      "type": "string"
    },
    "cohortCriteria": {
      "title": "t(initiative.cohortCriteria.title)",
      "type": "string"
    },
    "socioEnvContext": {
      "title": "t(initiative.socioEnvContext.title)",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "air_pollution",
          "climate_change",
          "environmental_hazards",
          "short_half_life_chemicals",
          "urbanization",
          "work_related_stress",
          "work_life_balance",
          "migration",
          "vnps",
          "internet_use",
          "social_media_use",
          "gaming_online",
          "gaming_offline",
          "other",
          "na"
        ]
      }
    },
    "socioEnvContextOther": {
      "title": "t(initiative.socioEnvContextOther.title)",
      "type": "string"
    },
    "nbParticipants": {
      "type": "object",
      "properties": {
        "total": {
          "type": "number",
          "title": "t(initiative.nbParticipantsTotal.title)"
        },
        "harmonized": {
          "type": "number",
          "title": "t(initiative.nbParticipantsHarmonized.title)"
        }
      }
    },
    "age": {
      "type": "object",
      "properties": {
        "min": {
          "type": "number",
          "title": "t(initiative.ageMin.title)"
