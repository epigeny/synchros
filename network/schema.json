        "max": {
          "type": "number",
          "title": "t(initiative.ageMax.title)"
        }
      }
    },
    "methodology": {
      "type": "object",
      "properties": {
        "harmonizationStrategy": {
          "type": "string",
          "title": "t(initiative.harmonizationStrategy.title)",
